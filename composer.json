{
    "name": "aprchen/csv-helper",
    "description": "协程读取,处理速度快,可以识别编码",
    "type": "library",
    "license": "Apache-2.0",
    "keywords": ["csv", "import", "export", "read", "write", "filter"],
    "authors": [
        {
            "name": "ott321",
            "email": "ott321@yeah.net"
        }
    ],
    "require": {
        "php" : ">=7.0.10",
        "ext-mbstring" : "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Aprchen\\CsvHelper\\": "src"
        },
        "files": ["src/Helper/Functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "AprchenTest\\CsvHelper\\": "test"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit -c phpunit.xml"
    }
}
